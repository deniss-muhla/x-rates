{
  "//": [
    "********************************************************************",
    "scripts:",
    "--------------------------------------------------------------------",
    "start development environment with cypress UI:",
    "> yarn dev",
    "--------------------------------------------------------------------",
    "start development environment without tests:",
    "> yarn start",
    "--------------------------------------------------------------------",
    "start development environment and run tests with headless cypress:",
    "> yarn test",
    "--------------------------------------------------------------------",
    "make production build:",
    "> yarn build",
    "--------------------------------------------------------------------",
    "make release version:",
    "> yarn release",
    "--------------------------------------------------------------------",
    "open cypress UI:",
    "> yarn cy:open",
    "--------------------------------------------------------------------",
    "run tests with headless cypress:",
    "> yarn cy:run",
    "--------------------------------------------------------------------",
    "commit message wizard:",
    "> yarn commit",
    "********************************************************************"
  ],
  "name": "x-rates",
  "version": "1.1.2",
  "private": true,
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "@material-ui/core": "^4.5.1",
    "axios": "^0.21.1",
    "clsx": "^1.0.4",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/node": "12.7.11",
    "@types/react": "16.9.5",
    "@types/react-dom": "16.9.1",
    "@types/react-redux": "^7.1.4",
    "@types/webpack-env": "^1.14.0",
    "commitizen": "^4.0.3",
    "cypress": "^3.4.1",
    "cypress-skip-and-only-ui": "^1.2.4",
    "cypress-watch-and-reload": "^1.2.11",
    "husky": "^3.0.8",
    "react-scripts": "3.2.0",
    "standard-version": "^7.0.0",
    "start-server-and-test": "^1.10.4",
    "typescript": "3.6.3"
  },
  "scripts": {
    "cy:open": "cypress open --config baseUrl=http://localhost:3000",
    "cy:run": "cypress run --config baseUrl=http://localhost:3000",
    "dev": "start-test 3000 cy:open",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "release": "standard-version",
    "now-build": "yarn build",
    "test": "start-test 3000 cy:run",
    "eject": "react-scripts eject",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
